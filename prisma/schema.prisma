

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Imovel{
  id Int @id @default(autoincrement())
  quartos Int
  texto String
  valor Float
  imagem Imagem[]
  bairro String
  rua String
  cidade String
  estado String
  numeroCasa Int
  nome String
  cep Int
}

model Imagem{
  id Int @id @default(autoincrement())
  imagem String
  imovel Imovel@relation(fields: [imovel_id], references: [id], onDelete: Cascade )
  imovel_id Int

}